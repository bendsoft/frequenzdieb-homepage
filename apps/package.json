{
  "name": "apps",
  "version": "0.0.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "ng": "ng",
    "start:ticketing-api": "ng build --project=ticketing-api --watch",
    "start:ui": "ng serve --project=ui --port 4201",
    "start:bouncer": "ng serve --project=bouncer --port 4202 --configuration=de-CH",
    "start:bouncer-prod": "ng build --project=bouncer --configuration production --localize && http-server -p 8086 -c-1 dist/bouncer",
    "build": "ng build",
    "build:ticketing-api": "ng build --project=ticketing-api",
    "publish:ticketing-api": "npm publish dist/ticketing-api",
    "build:ui": "ng build --project=ui",
    "build:ui-prod": "ng build --project=ui --configuration production",
    "build:bouncer": "ng build --project=bouncer",
    "build:bouncer-prod": "ng build --project=bouncer --configuration production --localize",
    "test": "ng test",
    "test:ui": "ng test --project=ui",
    "test:bouncer": "ng test --project=bouncer",
    "lint": "eslint projects/*/src/**/*.[tj]s",
    "lint:ticketing-api": "eslint projects/*/src/**/*.[tj]s --project=ticketing-api",
    "lint:ui": "eslint projects/*/src/**/*.[tj]s --project=ui",
    "lint:bouncer": "eslint projects/*/src/**/*.[tj]s --project=bouncer",
    "e2e": "ng e2e",
    "e2e:ui": "ng e2e --project=ui",
    "e2e:bouncer": "ng e2e --project=bouncer"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.3",
    "@angular/cdk": "~13.0.3",
    "@angular/common": "~13.0.3",
    "@angular/compiler": "~13.0.3",
    "@angular/core": "~13.0.3",
    "@angular/forms": "~13.0.3",
    "@angular/localize": "~13.0.3",
    "@angular/material": "~13.0.3",
    "@angular/platform-browser": "~13.0.3",
    "@angular/platform-browser-dynamic": "~13.0.3",
    "@angular/platform-server": "~13.0.3",
    "@angular/router": "~13.0.3",
    "@angular/service-worker": "~13.0.3",
    "@bendsoft/ticketing-api": "~0.0.10",
    "@zxing/ngx-scanner": "~3.0.0",
    "lodash-es": "~4.17.15",
    "ng-recaptcha": "~7.0.1",
    "rxjs": "~6.6.0",
    "tslib": "~2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.4",
    "@angular/cli": "~13.0.4",
    "@angular/compiler-cli": "~13.0.3",
    "@angular/language-service": "~13.0.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.11.1",
    "@typescript-eslint/eslint-plugin": "~2.30.0",
    "@typescript-eslint/parser": "~2.27.0",
    "codelyzer": "~6.0.1",
    "eslint": "~6.8.0",
    "eslint-config-airbnb-typescript": "~7.2.0",
    "eslint-config-prettier": "~6.10.1",
    "eslint-import-resolver-alias": "~1.1.2",
    "eslint-plugin-import": "~2.20.2",
    "eslint-plugin-prettier": "~3.1.2",
    "http-server": "~0.12.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "ng-packagr": "~13.0.8",
    "prettier": "~2.0.5",
    "prettier-eslint": "~9.0.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.2",
    "typescript": "~4.4.4"
  }
}
